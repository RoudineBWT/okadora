// ────────────── Startup Applications ──────────────
// https://github.com/YaLTeR/niri/wiki/Configuration:-Miscellaneous#spawn-sh-at-startup

//spawn-at-startup "/usr/lib/polkit-kde-authentication-agent-1" "&" // Polkit
spawn-at-startup "/usr/libexec/polkit-mate-authentication-agent-1" "&" // Polkit
spawn-at-startup "xwayland-satellite" // XWayland support
//spawn-at-startup "mpvpaper" "-vp" "-o" "no-audio loop" "DP-1" "~/backgrounds/kid-goku-and-shenron-dragon-ball-moewalls-com.mp4"
spawn-at-startup "swww-daemon" // Wallpaper daemon
//spawn-at-startup "swww img" "/usr/share/wallpapers/cachyos-wallpapers/Skyscraper.png" // Set wallpaper
spawn-at-startup "qs" "-c" "noctalia-shell" // Launch Quickshell
spawn-at-startup "flatpak" "run" "dev.vencord.Vesktop" // Launch Discord
spawn-at-startup "openrgb" // Launch OpenRGB
//spawn-at-startup "Telegram" // Launch Telegram
spawn-at-startup "niri" "msg" "action" "show-hotkey-overlay" // Show Hotkey Overlay
